<AnchorPane prefHeight="608.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jdictionaryev1.OlympiaController">
   <children>
      <ImageView fitHeight="616.0" fitWidth="1099.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../GameOlympia/11.png" />
         </image>
      </ImageView>
      <TextField fx:id="myAnswer" alignment="CENTER" disable="true" layoutX="369.0" layoutY="363.0" prefHeight="56.0" prefWidth="299.0" promptText="My Answer" styleClass="text-field-Olympia" stylesheets="@../../../application.css">
         <font>
            <Font name="Impact" size="36.0" />
         </font>
      </TextField>
      <Label fx:id="Answer" layoutX="522.0" layoutY="467.0" prefHeight="24.0" prefWidth="139.0" text="Answer" textFill="#c90b0b" visible="false">
         <font>
            <Font name="Impact" size="20.0" />
         </font>
      </Label>
      <Label fx:id="score" layoutX="1017.0" layoutY="158.0" text="Score" visible="false">
         <font>
            <Font name="Impact" size="20.0" />

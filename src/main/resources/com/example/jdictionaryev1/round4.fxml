            <Font name="Impact" size="22.0" />
         </font>
      </Label>
      <Label fx:id="labelPackage2" alignment="CENTER" layoutX="968.0" layoutY="272.0" prefHeight="47.0" prefWidth="51.0" text="10" textFill="WHITE" visible="false">
         <font>
            <Font name="Impact" size="22.0" />
         </font>
      </Label>
      <Label fx:id="labelPackage3" alignment="CENTER" layoutX="1023.0" layoutY="272.0" prefHeight="47.0" prefWidth="51.0" text="10" textFill="WHITE" visible="false">
         <font>
            <Font name="Impact" size="22.0" />
         </font>
      </Label>

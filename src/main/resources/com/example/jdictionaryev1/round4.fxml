            <Font size="29.0" />
         </font>
      </Button>
      <Button fx:id="endGameButton" layoutX="1013.0" layoutY="532.0" mnemonicParsing="false" onAction="#endGame" prefHeight="63.0" prefWidth="68.0" style="-fx-background-radius: 60;" styleClass="out" stylesheets="@../../../application.css">

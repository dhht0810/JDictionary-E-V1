            <ImageView fitHeight="54.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../GameOlympia/outGame.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="resultButton" layoutX="423.0" layoutY="255.0" mnemonicParsing="false" onAction="#gotoResult" prefHeight="36.0" prefWidth="246.0" styleClass="choice_Game" stylesheets="@../../../application.css" text="End Game" visible="false">

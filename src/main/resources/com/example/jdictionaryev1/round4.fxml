            <Font name="Impact" size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="correctImage" fitHeight="40.0" fitWidth="48.0" layoutX="782.0" layoutY="343.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../../GameOlympia/correct.png" />
         </image>
      </ImageView>
      <ImageView fx:id="wrongImage" fitHeight="40.0" fitWidth="48.0" layoutX="782.0" layoutY="396.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../../GameOlympia/wrong.png" />
         </image>
      </ImageView>
      <Label fx:id="countdownLabel4" layoutX="97.0" layoutY="311.0" prefHeight="108.0" prefWidth="307.0" text="15" textFill="#fff9f9">
         <font>
            <Font name="Rockwell Condensed" size="72.0" />
         </font>
      </Label>
      <Button fx:id="countdownButton" layoutX="60.0" layoutY="416.0" mnemonicParsing="false" onAction="#startCountDownRound1" prefHeight="62.0" prefWidth="148.0" text="Start">

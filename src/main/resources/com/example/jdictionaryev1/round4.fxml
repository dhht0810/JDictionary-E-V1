         <graphic>
            <ImageView fitHeight="63.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../GameOlympia/energetic.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fx:id="starHopeImage" fitHeight="107.0" fitWidth="111.0" layoutX="796.0" layoutY="199.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../../GameOlympia/star.png" />
         </image>
      </ImageView>
